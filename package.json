{
  "name": "deduce",
  "version": "0.0.0-semantic-release",
  "description": "Composable state containers.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/shannonmoeller/deduce.git"
  },
  "homepage": "https://github.com/shannonmoeller/deduce#readme",
  "author": {
    "name": "Shannon Moeller",
    "email": "me@shannonmoeller.com",
    "url": "http://shannonmoeller.com"
  },
  "main": "lib/deduce.js",
  "files": [
    "lib"
  ],
  "dependencies": {},
  "devDependencies": {
    "ava": "^0.18.1",
    "babel-cli": "^6.14.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-2": "^6.13.0",
    "babel-register": "^6.14.0",
    "chokidar-cli": "^1.2.0",
    "coveralls": "^2.11.12",
    "nyc": "^8.1.0",
    "rimraf": "^2.5.4",
    "semantic-release": "^4.3.5",
    "xo": "^0.16.0"
  },
  "scripts": {
    "coveralls": "nyc report -r text-lcov | coveralls",
    "prepublish": "rimraf lib && babel src --out-dir lib",
    "pretest": "xo '*.js' '{src,test}/**/*.js'",
    "release": "semantic-release pre && npm publish && semantic-release post",
    "test": "nyc -r lcov -r text ava -v",
    "watch": "chokidar '{src,test}/**/*.js' -c 'npm test'"
  },
  "engines": {
    "node": ">= 0.12"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "nyc": {
    "require": [
      "babel-polyfill",
      "babel-register"
    ]
  },
  "xo": {
    "rules": {
      "brace-style": [
        2,
        "stroustrup"
      ],
      "eqeqeq": [
        2,
        "allow-null"
      ],
      "no-eq-null": 0,
      "object-curly-spacing": [
        2,
        "always"
      ],
      "operator-linebreak": [
        2,
        "before"
      ],
      "babel/object-curly-spacing": [
        2,
        "always"
      ]
    }
  }
}
